{
  "ReverseProxy": {
    "Routes": {
      "authService": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "authService": {
            "Address": "http://localhost:7009"
          }
        }
      }
    }
  }
}

